$("#all_images").html("<%= escape_javascript(render 'all_images') %>");
document.querySelector("#all_images").innerHTML = "<%= escape_javascript(c("all_images", id_column: id_column, column_id: column_id)) %>";



$(document).ready(function () {
  $("#<%= id_column %>").on("click", function () {
    $("div#<%= column_id %>").addClass("is-active");
  })
  $("#close, div.modal-background").on("click", function () {
    $("div#<%= column_id %>").removeClass("is-active");
  })
});
